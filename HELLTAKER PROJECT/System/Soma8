.text
li a7,5 #ler int
ecall
mv s0,a0

li a7,5 #ler int
ecall
mv s1,a0

mv t0,s0 
li s2,8
li t2,0
SOMAX:

loop:
bgt t0,s1,done
rem t1,t0,s2
bnez t1,vira8
add t2,t2,t0
addi t0,t0,8
j loop

vira8:
addi t0,t0,1
j loop

done:
li a7, 1
mv a0,t2			        # serviço de print int
ecall		
li a7, 10
ecall


